<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd"
	default-autowire="byName">


	<alias name="simpleMemeGenerator" alias="memeGenerator"/>
	<bean id="simpleMemeGenerator" class="org.funny.pictures.generator.BaseAdviceMemeGenerator">
		<property name="outputFormat" value="IMAGE_PNG_PNG" />
		<property name="imageInfoExtractor" ref="imageInformationExtractor"/>
	</bean>
	
	<bean id="imageInformationExtractor" class="org.funny.pictures.generator.util.ImageInformationExtractor"/>

</beans>